programa
{
	
	funcao inicio()
	{
		inteiro termoA = 0
		inteiro termoB = 1
		inteiro termoC 
		inteiro n

		escreva("Informe o n-simo termo: \n")
		leia(n)

		escreva(termoA, " ", termoB, " ")

		para(termoC = 0; termoC<=n; termoC++){
					termoC = termoA + termoB
					termoA = termoB
					termoB = termoC
					escreva(termoC , " ")
			}
	}
}

